{
    "name": "dennis-koster/eloquent-mock",
    "description": "This package aims to make mocking eloquent models easier.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "mockery/mockery": "^1",
        "laravel/framework": "^10|^11"
    },
    "autoload": {
        "psr-4": {
            "DennisKoster\\EloquentMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DennisKoster\\EloquentMock\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
